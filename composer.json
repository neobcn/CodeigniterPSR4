{
    "name": "neobcn/hello-world",
    "description": "CodeIgniter 3 PSR-4 Autoloader for Application",
    "keywords": ["codeIgniter", "autoload", "PSR-4", "autoloader"],
    "homepage": "https://github.com/neobcn/hello-world",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/neobcn/hello-world/issues",
        "source": "https://github.com/neobcn/hello-world"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "classmap": ["src/"],
        "psr-4": {
            "App\\": "../../../"
        }
    }
}
